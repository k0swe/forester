{
  "name": "forester",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "prebuild-prod": "ts-node -O '{\"module\": \"commonjs\"}' git-version.ts",
    "build-prod": "ng build --configuration production",
    "test": "firebase emulators:exec --project k0swe-kellog 'ng test'",
    "test-ci": "firebase emulators:exec --project k0swe-kellog --only firestore 'ng test --no-watch --no-progress --browsers=ChromeHeadlessCI'",
    "lint": "ng lint",
    "license-report": "license-report --config license-report-config.json > src/assets/license-report.json && git add src/assets/license-report.json"
  },
  "private": true,
  "browser": {
    "fs": false,
    "net": false,
    "os": false,
    "path": false,
    "tls": false
  },
  "engines": {
    "node": "20"
  },
  "dependencies": {
    "@amrato/maidenhead-ts": "2.0.0",
    "@angular/animations": "18.1.0",
    "@angular/cdk": "18.1.0",
    "@angular/common": "18.1.0",
    "@angular/compiler": "18.1.0",
    "@angular/core": "18.1.0",
    "@angular/fire": "18.0.1",
    "@angular/forms": "18.1.0",
    "@angular/google-maps": "18.1.0",
    "@angular/localize": "18.1.0",
    "@angular/material": "18.1.0",
    "@angular/platform-browser": "18.1.0",
    "@angular/platform-browser-dynamic": "18.1.0",
    "@angular/router": "18.1.0",
    "@angular/service-worker": "18.1.0",
    "@googlemaps/js-api-loader": "1.16.6",
    "@types/long": "5.0.0",
    "adif-parser-ts": "0.6.1",
    "firebase": "10.12.3",
    "google-protobuf": "3.21.4",
    "js-joda": "1.11.0",
    "json-ptr": "3.1.1",
    "lodash.isequal": "4.5.0",
    "moment": "2.30.1",
    "ngx-kel-agent": "0.7.0",
    "rxjs": "7.8.1",
    "ts-protoc-gen": "0.15.0",
    "tslib": "2.6.3",
    "zone.js": "0.14.7"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1801.0",
    "@angular-devkit/build-angular": "18.1.0",
    "@angular-eslint/builder": "18.1.0",
    "@angular-eslint/eslint-plugin": "18.1.0",
    "@angular-eslint/eslint-plugin-template": "18.1.0",
    "@angular-eslint/schematics": "18.1.0",
    "@angular-eslint/template-parser": "18.1.0",
    "@angular/cli": "18.1.0",
    "@angular/compiler-cli": "18.1.0",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/google.maps": "3.55.11",
    "@types/jasmine": "5.1.4",
    "@types/jasminewd2": "2.0.13",
    "@types/lodash.isequal": "4.5.8",
    "@types/node": "20.14.11",
    "@typescript-eslint/eslint-plugin": "7.16.0",
    "@typescript-eslint/parser": "7.16.0",
    "eslint": "8.57.0",
    "fuzzy": "0.1.3",
    "inquirer": "9.3.5",
    "inquirer-autocomplete-prompt": "3.0.1",
    "jasmine-core": "5.1.2",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.3",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "license-report": "6.5.0",
    "lint-staged": "15.2.7",
    "open": "10.1.0",
    "prettier": "3.3.3",
    "ts-node": "10.9.2",
    "typescript": "5.4.5"
  },
  "lint-staged": {
    "*.{js,ts,json,html,css,scss,md,yml,yaml,webmanifest}": "prettier --write"
  },
  "prettier": {
    "singleQuote": true,
    "importOrder": [
      "<THIRD_PARTY_MODULES>",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrderParserPlugins": [
      "typescript",
      "decorators-legacy"
    ],
    "plugins": [
      "@trivago/prettier-plugin-sort-imports"
    ]
  }
}
