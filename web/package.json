{
  "name": "forester",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "prebuild-prod": "ts-node -O '{\"module\": \"commonjs\"}' git-version.ts",
    "build-prod": "ng build --configuration production",
    "test": "firebase emulators:exec --project k0swe-kellog 'ng test'",
    "test-ci": "firebase emulators:exec --project k0swe-kellog --only firestore 'ng test --no-watch --no-progress --browsers=ChromeHeadlessCI'",
    "lint": "ng lint",
    "license-report": "license-report --config license-report-config.json > src/assets/license-report.json && git add src/assets/license-report.json"
  },
  "private": true,
  "browser": {
    "fs": false,
    "net": false,
    "os": false,
    "path": false,
    "tls": false
  },
  "engines": {
    "node": ">= 22"
  },
  "dependencies": {
    "@amrato/maidenhead-ts": "2.0.0",
    "@angular/animations": "20.1.4",
    "@angular/cdk": "20.1.4",
    "@angular/common": "20.1.4",
    "@angular/compiler": "20.1.4",
    "@angular/core": "20.1.4",
    "@angular/fire": "20.0.1",
    "@angular/forms": "20.1.4",
    "@angular/google-maps": "20.1.4",
    "@angular/localize": "20.1.4",
    "@angular/material": "20.1.4",
    "@angular/platform-browser": "20.1.4",
    "@angular/platform-browser-dynamic": "20.1.4",
    "@angular/router": "20.1.4",
    "@angular/service-worker": "20.1.4",
    "@googlemaps/js-api-loader": "1.16.10",
    "adif-parser-ts": "0.6.24",
    "firebase": "12.0.0",
    "js-joda": "1.11.0",
    "lodash-es": "4.17.21",
    "moment": "2.30.1",
    "ngx-kel-agent": "1.2.1",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.2001.4",
    "@angular-eslint/builder": "20.1.1",
    "@angular-eslint/eslint-plugin": "20.1.1",
    "@angular-eslint/eslint-plugin-template": "20.1.1",
    "@angular-eslint/schematics": "20.1.1",
    "@angular-eslint/template-parser": "20.1.1",
    "@angular/build": "^20.1.4",
    "@angular/cli": "20.1.4",
    "@angular/compiler-cli": "20.1.4",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@types/google.maps": "3.58.1",
    "@types/jasmine": "5.1.8",
    "@types/jasminewd2": "2.0.13",
    "@types/lodash.isequal": "4.5.8",
    "@types/node": "24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "jasmine-core": "5.9.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "license-report": "6.8.0",
    "lint-staged": "16.1.4",
    "prettier": "3.6.2",
    "ts-node": "10.9.2",
    "typescript": "5.8.3"
  },
  "lint-staged": {
    "*.{js,ts,json,html,css,scss,md,yml,yaml,webmanifest}": "prettier --write"
  },
  "prettier": {
    "singleQuote": true,
    "importOrder": [
      "<THIRD_PARTY_MODULES>",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrderParserPlugins": [
      "typescript",
      "decorators-legacy"
    ],
    "plugins": [
      "@trivago/prettier-plugin-sort-imports"
    ]
  }
}
