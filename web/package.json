{
  "name": "forester",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "prebuild-prod": "ts-node -O '{\"module\": \"commonjs\"}' git-version.ts",
    "build-prod": "ng build --configuration production",
    "test": "firebase emulators:exec 'ng test'",
    "test-ci": "firebase emulators:exec --only firestore 'ng test --no-watch --no-progress --browsers=ChromeHeadlessCI'",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "license-report": "license-report --config license-report-config.json > src/assets/license-report.json && git add src/assets/license-report.json"
  },
  "private": true,
  "browser": {
    "fs": false,
    "net": false,
    "os": false,
    "path": false,
    "tls": false
  },
  "dependencies": {
    "@amrato/maidenhead-ts": "2.0.0",
    "@angular/animations": "15.2.9",
    "@angular/cdk": "15.2.9",
    "@angular/common": "15.2.9",
    "@angular/compiler": "15.2.9",
    "@angular/core": "15.2.9",
    "@angular/fire": "7.5.0",
    "@angular/forms": "15.2.9",
    "@angular/google-maps": "15.2.9",
    "@angular/localize": "15.2.9",
    "@angular/material": "15.2.9",
    "@angular/platform-browser": "15.2.9",
    "@angular/platform-browser-dynamic": "15.2.9",
    "@angular/router": "15.2.9",
    "@angular/service-worker": "15.2.9",
    "@types/long": "4.0.2",
    "adif-parser-ts": "0.4.75",
    "firebase": "9.21.0",
    "google-protobuf": "3.21.2",
    "js-joda": "1.11.0",
    "json-ptr": "3.1.1",
    "lodash.isequal": "4.5.0",
    "moment": "2.29.4",
    "ngx-kel-agent": "0.5.67",
    "rxfire": "6.0.3",
    "rxjs": "7.8.1",
    "ts-protoc-gen": "0.15.0",
    "tslib": "2.5.0",
    "zone.js": "0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1600.0",
    "@angular-devkit/build-angular": "15.2.8",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "15.2.8",
    "@angular/compiler-cli": "15.2.9",
    "@google-cloud/firestore": "6.5.0",
    "@types/jasmine": "4.3.1",
    "@types/jasminewd2": "2.0.10",
    "@types/lodash.isequal": "4.5.6",
    "@types/node": "18.15.11",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "dedent": "0.7.0",
    "eslint": "8.40.0",
    "firebase-admin": "11.7.0",
    "firebase-tools": "11.29.1",
    "fuzzy": "0.1.3",
    "inquirer": "9.2.2",
    "inquirer-autocomplete-prompt": "3.0.0",
    "jasmine-core": "4.6.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.2",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "license-report": "6.4.0",
    "lint-staged": "13.2.2",
    "open": "9.1.0",
    "prettier": "2.8.8",
    "protractor": "7.0.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "lint-staged": {
    "*.{js,ts,json,html,css,scss,md,yml,yaml,webmanifest}": "prettier --write"
  },
  "prettier": {
    "singleQuote": true
  }
}
