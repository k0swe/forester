{
  "name": "forester",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "prebuild-prod": "ts-node -O '{\"module\": \"commonjs\"}' git-version.ts",
    "build-prod": "ng build --configuration production",
    "test": "firebase emulators:exec --project k0swe-kellog 'ng test'",
    "test-ci": "firebase emulators:exec --project k0swe-kellog --only firestore 'ng test --no-watch --no-progress --browsers=ChromeHeadlessCI'",
    "lint": "ng lint",
    "license-report": "license-report --config license-report-config.json > src/assets/license-report.json && git add src/assets/license-report.json"
  },
  "private": true,
  "browser": {
    "fs": false,
    "net": false,
    "os": false,
    "path": false,
    "tls": false
  },
  "engines": {
    "node": "24"
  },
  "dependencies": {
    "@amrato/maidenhead-ts": "2.0.0",
    "@angular/animations": "21.1.2",
    "@angular/cdk": "21.1.2",
    "@angular/common": "21.1.2",
    "@angular/compiler": "21.1.2",
    "@angular/core": "21.1.2",
    "@angular/fire": "^21.0.0-rc.0",
    "@angular/forms": "21.1.2",
    "@angular/google-maps": "21.1.2",
    "@angular/localize": "21.1.2",
    "@angular/material": "21.1.2",
    "@angular/platform-browser": "21.1.2",
    "@angular/platform-browser-dynamic": "21.1.2",
    "@angular/router": "21.1.2",
    "@angular/service-worker": "21.1.2",
    "@googlemaps/js-api-loader": "2.0.2",
    "@types/long": "5.0.0",
    "adif-parser-ts": "0.6.31",
    "firebase": "12.8.0",
    "google-protobuf": "4.0.1",
    "js-joda": "1.11.0",
    "json-ptr": "3.1.1",
    "lodash.isequal": "4.5.0",
    "moment": "2.30.1",
    "ngx-kel-agent": "1.2.11",
    "rxjs": "7.8.2",
    "ts-protoc-gen": "0.15.0",
    "tslib": "2.8.1",
    "zone.js": "0.16.0"
  },
  "devDependencies": {
    "@angular-eslint/builder": "21.1.0",
    "@angular-eslint/eslint-plugin": "21.1.0",
    "@angular-eslint/eslint-plugin-template": "21.1.0",
    "@angular-eslint/schematics": "21.1.0",
    "@angular-eslint/template-parser": "21.1.0",
    "@angular/build": "^21.1.0",
    "@angular/cli": "21.1.2",
    "@angular/compiler-cli": "21.1.2",
    "@trivago/prettier-plugin-sort-imports": "6.0.2",
    "@types/google.maps": "3.58.1",
    "@types/jasmine": "5.1.15",
    "@types/jasminewd2": "2.0.13",
    "@types/lodash.isequal": "4.5.8",
    "@types/node": "25.0.10",
    "@typescript-eslint/eslint-plugin": "8.54.0",
    "@typescript-eslint/parser": "8.54.0",
    "eslint": "9.39.2",
    "jasmine-core": "5.13.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "license-report": "6.8.1",
    "lint-staged": "16.2.7",
    "open": "11.0.0",
    "prettier": "3.8.1",
    "ts-node": "10.9.2",
    "typescript": "5.9.3"
  },
  "lint-staged": {
    "*.{js,ts,json,html,css,scss,md,yml,yaml,webmanifest}": "prettier --write"
  },
  "prettier": {
    "singleQuote": true,
    "importOrder": [
      "<THIRD_PARTY_MODULES>",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrderParserPlugins": [
      "typescript",
      "decorators-legacy"
    ],
    "plugins": [
      "@trivago/prettier-plugin-sort-imports"
    ]
  }
}
