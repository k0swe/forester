{
  "name": "forester",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --prod",
    "tag-deploy": "git tag deploy -m deploy -s --force && git push --tags --force",
    "test": "firebase emulators:exec 'ng test'",
    "test-ci": "firebase emulators:exec --only firestore 'ng test --no-watch --no-progress --browsers=ChromeHeadlessCI'",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prepare": "husky install"
  },
  "private": true,
  "browser": {
    "fs": false,
    "net": false,
    "os": false,
    "path": false,
    "tls": false
  },
  "dependencies": {
    "@angular/animations": "12.1.1",
    "@angular/cdk": "12.1.1",
    "@angular/common": "12.1.1",
    "@angular/compiler": "12.1.1",
    "@angular/core": "12.1.1",
    "@angular/fire": "6.1.5",
    "@angular/flex-layout": "12.0.0-beta.34",
    "@angular/forms": "12.1.1",
    "@angular/google-maps": "12.1.1",
    "@angular/localize": "12.1.1",
    "@angular/material": "12.1.1",
    "@angular/platform-browser": "12.1.1",
    "@angular/platform-browser-dynamic": "12.1.1",
    "@angular/router": "12.1.1",
    "@angular/service-worker": "12.1.1",
    "@types/long": "4.0.1",
    "adif-parser-ts": "0.3.41",
    "firebase": "8.7.1",
    "google-protobuf": "3.17.3",
    "moment": "2.29.1",
    "rxjs": "7.2.0",
    "ts-protoc-gen": "0.15.0",
    "tslib": "2.3.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1201.1",
    "@angular-devkit/build-angular": "12.1.1",
    "@angular-eslint/builder": "12.2.0",
    "@angular-eslint/eslint-plugin": "12.2.0",
    "@angular-eslint/eslint-plugin-template": "12.2.0",
    "@angular-eslint/schematics": "12.2.0",
    "@angular-eslint/template-parser": "12.2.0",
    "@angular/cli": "12.1.1",
    "@angular/compiler-cli": "12.1.1",
    "@firebase/rules-unit-testing": "1.3.10",
    "@google-cloud/firestore": "4.13.1",
    "@types/jasmine": "3.8.1",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "15.14.2",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "eslint": "7.30.0",
    "firebase-admin": "9.10.0",
    "firebase-tools": "9.16.0",
    "fuzzy": "0.1.3",
    "husky": "7.0.1",
    "inquirer": "8.1.1",
    "inquirer-autocomplete-prompt": "1.4.0",
    "jasmine-core": "3.8.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.6.0",
    "license-report": "4.5.0",
    "lint-staged": "11.0.0",
    "open": "8.2.1",
    "prettier": "2.3.2",
    "protractor": "7.0.0",
    "ts-node": "10.0.0",
    "typescript": "4.3.5"
  },
  "lint-staged": {
    "*.{js,ts,json,html,css,scss,md,yml,yaml,webmanifest}": "prettier --write"
  },
  "prettier": {
    "singleQuote": true
  }
}
