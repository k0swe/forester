    "license-report": "6.0.0",
    "lint-staged": "13.0.3",
    "open": "8.4.0",
    "prettier": "2.7.1",
    "protractor": "7.0.0",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "lint-staged": {
    "*.{js,ts,json,html,css,scss,md,yml,yaml,webmanifest}": "prettier --write"
  },
  "prettier": {
    "singleQuote": true
  }
}
