{
  "name": "forester",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --prod",
    "tag-deploy": "git tag deploy -m deploy -s --force && git push --tags --force",
    "test": "firebase emulators:exec 'ng test'",
    "test-ci": "firebase emulators:exec --only firestore 'ng test --no-watch --no-progress --browsers=ChromeHeadlessCI'",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prepare": "husky install"
  },
  "private": true,
  "browser": {
    "fs": false,
    "net": false,
    "os": false,
    "path": false,
    "tls": false
  },
  "dependencies": {
    "@angular/animations": "11.2.8",
    "@angular/cdk": "11.2.7",
    "@angular/common": "11.2.8",
    "@angular/compiler": "11.2.8",
    "@angular/core": "11.2.8",
    "@angular/fire": "6.1.4",
    "@angular/flex-layout": "10.0.0-beta.32",
    "@angular/forms": "11.2.8",
    "@angular/google-maps": "11.2.7",
    "@angular/localize": "11.2.8",
    "@angular/material": "11.2.7",
    "@angular/platform-browser": "11.2.8",
    "@angular/platform-browser-dynamic": "11.2.8",
    "@angular/router": "11.2.8",
    "@angular/service-worker": "11.2.8",
    "@types/long": "4.0.1",
    "adif-parser-ts": "0.3.26",
    "firebase": "8.3.2",
    "google-protobuf": "3.15.7",
    "moment": "2.29.1",
    "rxjs": "6.6.7",
    "ts-protoc-gen": "0.14.0",
    "tslib": "2.2.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1102.8",
    "@angular-devkit/build-angular": "0.1102.8",
    "@angular/cli": "11.2.8",
    "@angular/compiler-cli": "11.2.8",
    "@firebase/rules-unit-testing": "1.2.6",
    "@google-cloud/firestore": "4.9.8",
    "@types/jasmine": "3.6.9",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "14.14.37",
    "codelyzer": "6.0.1",
    "firebase-admin": "9.6.0",
    "firebase-tools": "9.9.0",
    "fuzzy": "0.1.3",
    "husky": "6.0.0",
    "inquirer": "8.0.0",
    "inquirer-autocomplete-prompt": "1.3.0",
    "jasmine-core": "3.7.1",
    "jasmine-spec-reporter": "6.0.0",
    "karma": "5.2.3",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "lint-staged": "10.5.4",
    "open": "8.0.5",
    "prettier": "2.2.1",
    "protractor": "7.0.0",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "typescript": "4.1.5"
  },
  "lint-staged": {
    "*.{js,ts,json,html,css,scss,md,yml,yaml,webmanifest}": "prettier --write"
  },
  "prettier": {
    "singleQuote": true
  }
}
