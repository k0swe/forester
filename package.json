{
  "name": "forester",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "prebuild-prod": "ts-node -O '{\"module\": \"commonjs\"}' git-version.ts",
    "build-prod": "ng build --configuration production",
    "test": "firebase emulators:exec 'ng test'",
    "test-ci": "firebase emulators:exec --only firestore 'ng test --no-watch --no-progress --browsers=ChromeHeadlessCI'",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prepare": "husky install"
  },
  "private": true,
  "browser": {
    "fs": false,
    "net": false,
    "os": false,
    "path": false,
    "tls": false
  },
  "dependencies": {
    "@amrato/maidenhead-ts": "2.0.0",
    "@angular/animations": "14.1.2",
    "@angular/cdk": "14.1.2",
    "@angular/common": "14.1.2",
    "@angular/compiler": "14.1.2",
    "@angular/core": "14.1.2",
    "@angular/fire": "7.4.1",
    "@angular/flex-layout": "14.0.0-beta.40",
    "@angular/forms": "14.1.2",
    "@angular/google-maps": "14.1.2",
    "@angular/localize": "14.1.2",
    "@angular/material": "14.1.2",
    "@angular/platform-browser": "14.1.2",
    "@angular/platform-browser-dynamic": "14.1.2",
    "@angular/router": "14.1.2",
    "@angular/service-worker": "14.1.2",
    "@types/long": "4.0.2",
    "adif-parser-ts": "0.4.45",
    "firebase": "9.9.2",
    "google-protobuf": "3.21.0",
    "js-joda": "1.11.0",
    "json-ptr": "3.1.1",
    "lodash.isequal": "4.5.0",
    "moment": "2.29.4",
    "ngx-kel-agent": "0.5.36",
    "rxfire": "6.0.3",
    "rxjs": "7.5.6",
    "ts-protoc-gen": "0.15.0",
    "tslib": "2.4.0",
    "zone.js": "0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1401.3",
    "@angular-devkit/build-angular": "14.1.3",
    "@angular-eslint/builder": "14.0.2",
    "@angular-eslint/eslint-plugin": "14.0.2",
    "@angular-eslint/eslint-plugin-template": "14.0.2",
    "@angular-eslint/schematics": "14.0.2",
    "@angular-eslint/template-parser": "14.0.2",
    "@angular/cli": "14.1.3",
    "@angular/compiler-cli": "14.1.2",
    "@firebase/rules-unit-testing": "2.0.4",
    "@google-cloud/firestore": "6.0.0",
    "@types/jasmine": "4.0.3",
    "@types/jasminewd2": "2.0.10",
    "@types/lodash.isequal": "4.5.6",
    "@types/node": "18.7.6",
    "@typescript-eslint/eslint-plugin": "5.33.1",
    "@typescript-eslint/parser": "5.33.1",
    "dedent": "0.7.0",
    "eslint": "8.22.0",
    "firebase-admin": "11.0.1",
    "firebase-tools": "11.7.0",
    "fuzzy": "0.1.3",
    "husky": "8.0.1",
    "inquirer": "8.2.4",
    "inquirer-autocomplete-prompt": "2.0.0",
    "jasmine-core": "4.3.0",
