{
  "name": "forester",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --prod",
    "deploy": "git tag deploy -m deploy -s --force && git push --tags --force",
    "test": "firebase emulators:exec 'ng test'",
    "test-ci": "firebase emulators:exec --only firestore 'ng test --no-watch --no-progress --browsers=ChromeHeadlessCI'",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "browser": {
    "fs": false,
    "net": false,
    "os": false,
    "path": false,
    "tls": false
  },
  "dependencies": {
    "@angular/animations": "11.2.1",
    "@angular/cdk": "11.2.1",
    "@angular/common": "11.2.1",
    "@angular/compiler": "11.2.1",
    "@angular/core": "11.2.1",
    "@angular/fire": "6.1.4",
    "@angular/flex-layout": "10.0.0-beta.32",
    "@angular/forms": "11.2.1",
    "@angular/google-maps": "11.2.1",
    "@angular/localize": "11.2.1",
    "@angular/material": "11.2.1",
    "@angular/platform-browser": "11.2.1",
    "@angular/platform-browser-dynamic": "11.2.1",
    "@angular/router": "11.2.1",
    "@angular/service-worker": "11.2.1",
    "@types/long": "4.0.1",
    "adif-parser-ts": "0.3.19",
    "firebase": "8.2.8",
    "google-protobuf": "3.15.0",
    "moment": "2.29.1",
    "rxjs": "6.6.3",
    "ts-protoc-gen": "0.14.0",
    "tslib": "2.1.0",
    "zone.js": "0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1102.1",
    "@angular-devkit/build-angular": "0.1102.1",
    "@angular/cli": "11.2.1",
    "@angular/compiler-cli": "11.2.1",
    "@firebase/rules-unit-testing": "1.2.2",
    "@google-cloud/firestore": "4.9.4",
    "@types/jasmine": "3.6.4",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "14.14.31",
    "codelyzer": "6.0.1",
    "firebase-admin": "9.5.0",
    "firebase-tools": "9.4.0",
    "fuzzy": "0.1.3",
    "husky": "4.3.8",
    "inquirer": "7.3.3",
    "inquirer-autocomplete-prompt": "1.3.0",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "6.0.0",
    "karma": "5.2.3",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "lint-staged": "10.5.4",
    "open": "7.4.2",
    "prettier": "2.2.1",
    "protractor": "7.0.0",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "typescript": "4.1.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,html,css,scss,md,yml,yaml,webmanifest}": "prettier --write"
  },
  "prettier": {
    "singleQuote": true
  }
}
